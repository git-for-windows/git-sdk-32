# See: man pkg-config
prefix=/mingw32
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Python
Description: Embed Python into an application
Requires:
Version: 3.10
Libs.private:  -lm -lversion -lshlwapi
Libs: -L${libdir} -lpython3.10
Cflags: -I${includedir}/python3.10
